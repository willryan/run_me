{
  "name": "run_me",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A Typescript library to quickly identify a task to run for a given file and line number",
  "scripts": {
    "test": "NODE_ENV=test mocha --require spec/setup.js --require ts-node/register",
    "testAll": "NODE_ENV=test mocha --require spec/setup.js --require ts-node/register 'spec/**/*.spec.ts*'"
  },
  "repository": "https://github.com/willryan/run_me",
  "engines": {
    "node": ">= 0.10.0"
  },
  "main": "lib/index.js",
  "typings": "lib/index",
  "files": [
    "lib/"
  ],
  "author": "Will Pleasant-Ryan",
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/mocha": "^2.2.32",
    "@types/node": "^7.0.4",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.12.1",
    "ts-loader": "^0.9.5",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.2",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "kexec": "^3.0.0"
  },
  "preferGlobal": true,
  "bin": {
    "run_me": "lib/index.js"
  }
}
